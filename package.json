{
  "private": true,
  "name": "atomiyama",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider preact build --dest docs --prerenderUrls ./prerender-urls.ts",
    "serve": "sirv docs --cors --single",
    "dev": "NODE_OPTIONS=--openssl-legacy-provider preact watch --prerenderUrls ./prerender-urls.ts",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "docs/"
    ]
  },
  "dependencies": {
    "@preact/prerender-data-provider": "^3.0.1",
    "markdown-to-jsx": "^7.2.0",
    "preact": "^10.10.0",
    "preact-render-to-string": "^5.2.1",
    "preact-router": "^3.2.1",
    "react-ga4": "^2.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.1",
    "eslint": "^8.20.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.4.0",
    "sirv-cli": "^3.0.1",
    "typescript": "^4.5.2"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.ts",
      "<rootDir>/tests/__mocks__/setupTests.ts"
    ]
  }
}
